{
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "import/no-default-export": "error",
    "comma-dangle": [
      "error",
      "never"
    ],
    "max-len": [
      "warn",
      {
        "code": 160,
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ]
  }
}
